{
  "apiVersion": "extensions/v1beta1",
  "kind": "Deployment",
  "metadata": {
    "name": "newrelic-deployment"
  },
  "spec": {
    "replicas": 1,
    "template": {
      "metadata": {
        "labels": {
          "app": "newrelic-collector",
          "tenant": ""
        }
      },
      "spec": {
        "containers": [
          {
            "name": "newrelic-collector-ctr",
            "image": "gcr.io/xops-poc/newrelic:1.3",
            "imagePullPolicy": "Always",
            "env": [
              {
                "name": "configs",
                "value": ""
              }
            ]
          }
        ]
      }
    }
  }
}
